@page
@model Rajwinder_shopping_centre.Pages.Signups.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>
<!--THIS PAGE IS USE FOR GET THE DETAILS OF SIGNUP PAGE-->

<div>
    <h4>Signup</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
         Signup Name
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Signup.Signup_Name)
        </dd>
        <dt class="col-sm-2">
         Signup Email
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Signup.Signup_Email)
        </dd>
        <dt class="col-sm-2">
         Signup Subject
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Signup.Signup_Subject)
        </dd>
        <dt class="col-sm-2">
         Signup Massage
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Signup.Signup_Massage)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.Signup.Id">Edit</a> |
    <a asp-page="./Index">Back to List</a>
</div>
