@page
@model Rajwinder_shopping_centre.Pages.Brands.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>
<!--THIS PAGE CREATE FOR GIVING THE DETAILS OF BRANDS NAME AND STATUS -->
<div>
    <h4>Brand</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
          Brand Name
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Brand.Brand_Name)
        </dd>
        <dt class="col-sm-2">
          Brand Status
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Brand.Brand_Status)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.Brand.Id">Edit</a> |
    <a asp-page="./Index">Back to List</a>
</div>
